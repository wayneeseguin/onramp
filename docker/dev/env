#!/usr/bin/env bash

export PG_USERNAME="postgres"
export PG_PASSWORD="onramp"
export PG_HOSTNAME="postgres"
export PG_DATABASE="postgres"
export PG_PORT="5432"
export PG_SCHEMA="onramp"
export PG_URI="${PG_URI:="postgres://${PG_USERNAME}:${PG_PASSWORD}@${PG_HOSTNAME}:${PG_PORT}/${PG_DATABASE}"}?options=--search-path%3D${PG_SCHEMA},public"

export APP_ROOT="/app"

